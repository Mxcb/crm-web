<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.crm.workbench.dao.transaction.TranHistoryDao">
    <insert id="insert">
        insert
        into
            tbl_tran_history (id, stage, money, tranId,expectedDate,createTime,createBy)
        values
            (#{id},#{stage},#{money},#{tranId},#{expectedDate},#{createTime},#{createBy})
    </insert>

    <select id="selectHistoryById" resultType="com.crm.workbench.domain.transaction.TranHistory">
        select *
        from tbl_tran_history
        where tranId=#{tranId};
    </select>
</mapper>

































