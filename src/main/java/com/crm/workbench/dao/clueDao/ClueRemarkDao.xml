<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.crm.workbench.dao.clueDao.ClueRemarkDao">
    <select id="selectListByClueId" resultType="com.crm.workbench.domain.clue.ClueRemark">
        select *
        from tbl_clue_remark
        where clueId=#{clueId};
    </select>

    <delete id="delete">
        delete from tbl_clue_remark where id=#{id}
    </delete>
</mapper>

































